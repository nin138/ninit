name: app
use: [ std ]
state: [ text: string, num: number ]
actions: [
    change: { text: string }
    inc: {}
    dec: {num: number}
]
reducer: {
    change: state.text = action.text
    inc: num = state.num + 1
    dec: num = state.num - action.num
}
props: []
child: [
    {
        type: std.HTML.input
        attribute: [
            type: text
        ]
        id: aaa
        parent: root
        child: [ bbb ]
        class: [ aaa, bbb ]
    },
]